convertt <- function(A)
{
  A$sample_summary$CI_upper_bounds <- A$sample_summary$CI_upperBounds
  A$sample_summary$CI_upperBounds <- NULL
  A$sample_summary$prob_essential_II <-  A$sample_summary$prob_essential
  A$sample_summary$prob_essential <- NULL
  A$sample_summary$prob_essential_I <-  A$sample_summary$prob_not_essential
  A$sample_summary$prob_not_essential <- NULL
  return(A)
}

data("resultsK562_high_extracted_output")
resultsK562_high_extracted_output <- convertt(resultsK562_high_extracted_output)
save(resultsK562_high_extracted_output,file="data/resultsK562_high_extracted_output.rda")


data("resultsK562_highest_extracted_output")
resultsK562_highest_extracted_output <- convertt(resultsK562_highest_extracted_output)
save(resultsK562_highest_extracted_output,file="data/resultsK562_highest_extracted_output.rda")

data("resultsK562_lowest_extracted_output")
resultsK562_lowest_extracted_output <- convertt(resultsK562_lowest_extracted_output)
save(resultsK562_lowest_extracted_output,file="data/resultsK562_lowest_extracted_output.rda")

data("resultsK562_highCas9_extracted_output")
resultsK562_highCas9_extracted_output <- convertt(resultsK562_highCas9_extracted_output)
save(resultsK562_highCas9_extracted_output,file="data/resultsK562_highCas9_extracted_output.rda")

